snippet fr "frozen string literal"
# frozen_string_literal: true
endsnippet

# メソッド系
snippet if
${1:CondStatement} ${2:Conditions}
	${3:Body}
end
endsnippet

snippet map
${1:Array}.map { |${2:Reciever}| ${3:Body} }
endsnippet

snippet each
${1:Array}.each do |${2:Reciever}|
	${3:Body}
end
endsnippet

