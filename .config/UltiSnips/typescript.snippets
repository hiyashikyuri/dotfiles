extends html

snippet log "log something"
console.log(${1:Content});
endsnippet

snippet fun "create a function"
const ${1:name} = (): ${2:Type} => {
}
endsnippet

snippet if "create a condition"
if (${1:condition}) {
	${2:Body}
}
endsnippet

snippet ife "create a condition"
if (${1:condition}) {
	${2:Body}
} else {
	${3:Body}
}
endsnippet

snippet interface "create an interface"
interface ${1:Name} {
	${2:Body}
}
endsnippet

snippet type "create an interface"
type ${1:Name} = {
	${2:Body}
}
endsnippet

