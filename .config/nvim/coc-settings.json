{
  "suggest.keepCompleteopt": false,
  // "flutter.fvm.enabled": true,
  // "coc.preferences.formatOnType": true,
  // NOTE(okubo): Dartの保存時にGoみたいに整形させる
  "coc.preferences.formatOnSaveFiletypes": ["dart"],
  // "pairs.enableCharacters": ["(", "[", "{", "`"],
  "pairs.enableBackspace": true,
  // RubyはcommandPathだけあれば良さそう
  "solargraph.commandPath": "~/.rbenv/shims/solargraph",
  //"suggest.enablePreselect": true,
  //"codeLens.enable": true,
  // "solargraph.useBundler": true,
  // NOTE(okubo): sourcekitはデフォルトでxcodeの方が実行されているが、その際にargsが飛んでいないので、そこ入れれたらUIKit動くはず
  "sourcekit.commandPath": "/Users/masahirookubo/development/sourcekit-lsp/.build/release/sourcekit-lsp",
  "sourcekit.args": [
    "-Xswiftc",
    "-sdk",
    "-Xswiftc",
    "/Applications/Xcode.app/Contents/Developer/Platforms/iPhoneSimulator.platform/Developer/SDKs/iPhoneSimulator15.2.sdk",
    "-Xswiftc",
    "-target",
    "-Xswiftc",
    "x86_64-apple-ios15.2-simulator"
  ],
  "languageserver": {
    "golang": {
      "command": "gopls",
      // "rootPatterns": ["go.mod"],
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      // "disableWorkspaceFolders": true,
      "filetypes": ["go"]
    },
    "kotlin": {
      // localにlsp設置する必要ありです
      "command": "/Users/masahirookubo/development/kotlin-language-server/server/build/install/server/bin/kotlin-language-server",
      "filetypes": ["kotlin"]
    },
    "nim": {
      "command": "nimlsp",
      "filetypes": ["nim"],
      "trace.server": "verbose"
    }
    // TODO(okubo): なぜかこちらなくても動く
    //"swift": {
     //  "filetypes": ["swift"],
      // "command": "/Users/masahirookubo/development/sourcekit-lsp/.build/release/sourcekit-lsp"
      //  "command": "/Users/masahirookubo/development/sourcekit-lsp/.build/release/sourcekit-lsp"
       // "command": "sourcekit-lsp"
      // "command": "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/bin/sourcekit-lsp"
   // }
  },
  // こちら追加することでDart lspの連携うまくいく
  "flutter.lsp.initialization.onlyAnalyzeProjectsWithOpenFiles": false,
  "eslint.packageManager": "yarn",
  "eslint.nodePath": ".yarn/sdks",
  "javascript.suggest.autoImports": true,
  "typescript.suggest.autoImports": true
}
