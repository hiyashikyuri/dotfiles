{
  "suggest.keepCompleteopt": false,
  // "coc.preferences.formatOnType": true,
  // NOTE(okubo): Dartの保存時にGoみたいに整形させる
  "coc.preferences.formatOnSaveFiletypes": [
    "dart"
  ],
  // "pairs.enableCharacters": ["(", "[", "{", "`"],
  "pairs.enableBackspace": true,
  // RubyはcommandPathだけあれば良さそう
  "solargraph.commandPath": "~/.rbenv/shims/solargraph",
  //"suggest.enablePreselect": true,
  //"codeLens.enable": true,
  // "solargraph.useBundler": true,
  "languageserver": {
    "golang": {
      "command": "gopls",
      // "rootPatterns": ["go.mod"],
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      // "disableWorkspaceFolders": true,
      "filetypes": ["go"]
    },
    "kotlin": {
      // localにlsp設置する必要ありです
      "command": "/Users/masahirookubo/development/kotlin-language-server/server/build/install/server/bin/kotlin-language-server",
      "filetypes": ["kotlin"]
    },
    "nim": {
      "command": "nimlsp",
      "filetypes": ["nim"],
      "trace.server": "verbose"
    }
  },
  // こちら追加することでDart lspの連携うまくいく
  "flutter.lsp.initialization.onlyAnalyzeProjectsWithOpenFiles": false,
  "eslint.packageManager": "yarn",
  "eslint.nodePath": ".yarn/sdks",
  "javascript.suggest.autoImports": true,
  "typescript.suggest.autoImports": true
}
